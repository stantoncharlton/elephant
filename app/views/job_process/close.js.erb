
$("#process_actions").find('#job_ready_to_close').remove().find('.bluebtn').data('tooltip').hide();
$('#job_closed_id').remove().find('.bluebtn').data('tooltip').hide();

$("#process_actions").append("<%= j render('job_process/job_closed', job_process: @job_process ) %>");

$('.job-progress').hide();

$('.activity-user-link').tooltip();
$('.tooltip-info').tooltip();


<% if @show_failures %>
$.ajax({
    url:"/failures?job_id=<%= @job_process.job.id %>",
    type:"GET",
    dataType:"script"
});
<% end %>

