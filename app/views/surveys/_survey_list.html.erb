<% last_point = nil %>
<% index = 0 %>
<div id="survey_list">
  <% points.each do |point| %>
      <%= render 'survey_points/survey_point', point: point, last_point: last_point %>
      <% last_point = point %>
      <% index += 1 %>
      <% if index % 5 == 0 %>
          <br>
      <% end %>
  <% end %>
</div>