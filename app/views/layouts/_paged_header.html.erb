<% if items.count > 0 %>
    <% if params[:page].present? %>
        <%= ((Integer(params[:page]) - 1) * 10) + 1 %> - <%= [((Integer(params[:page]) - 1) * 10) + 10, items.count].min %> of <%= items.count %>
    <% else %>
        1 - <%= [10, items.count].min %> of <%= items.count %>
    <% end %>
<% end %>