<%= tabs_tag :builder => ApplicationHelper::MenuTabBuilder, open_tabs: {class: "admin_tab"} do |tab| %>

    <%= tab.jobs "Drilling", jobs_path %>




    <%= tab.alarms "Alarms", inventory_index_path %>

    <%= tab.incidents "Incidents", issues_path %>

    <% if current_user.role.global_read? %>
        <%= tab.insight "Trends", insight_path %>
    <% else %>
        <%= tab.alerts "Agenda", alerts_path %>
    <% end %>

<% end %>
