<h1 class="page-title">Create New Job Type</h1>

<%= form_for(@job_template) do |f| %>

    <%= render 'shared/error_messages', object: f.object %>

    <%= render 'fields', f: f %>


    <div class="actions">
      <%= f.submit "Create Job type", class: "bluebtn pull-right" %>
    </div>

<% end %>





