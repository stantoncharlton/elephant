<div class="page_header">
  <h1><%= t ".title" %></h1>
</div>

<%= form_for(@job_template) do |f| %>

    <%= render 'shared/error_messages', object: f.object %>


    <%= render 'fields', f: f %>

    <div class="actions">
      <%= f.submit I18n.t("job_templates.new.save"), class: "bluebtn pull-right" %>
    </div>



<% end %>





