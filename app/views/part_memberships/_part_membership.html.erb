<div id="part_membership_<%= part_membership.id %>" class="field">
  <div>
    <b class='blue-text'>    <%= part_membership.material_number %>
    </b>
    <p class="help-text inline"> - Asset Material #</p>

    <%= link_to "remove", part_membership, method: :delete, remote: true, data: {confirm: "Remove Asset?"},
                class: "pull-right delete-button tooltip-info",
                "data-placement" => "bottom",
                "data-title" => "Delete asset" %>

  </div>
</div>

