<% if @document.new_record? %>
alert("Failed to upload document: <%= j @document.errors.full_messages.join(', ').html_safe %>");
<% else %>

<% if @document.category == "Pre-Job" %>
    $("#documents").append("<%= j render(@document) %>");
<% else %>
    $("#documents2").append("<%= j render(@document) %>");
<% end %>

$('#new_document_name').val('');


<% end %>
